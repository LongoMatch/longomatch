msgid "Surround"
msgstr "Térhatású"

#: ../libcesarplayer/bacon-video-widget-gst-0.10.c:4601
msgid "Mono"
msgstr "Mono"

#: ../libcesarplayer/bacon-video-widget-gst-0.10.c:4603
msgid "Stereo"
msgstr "Sztereó"

